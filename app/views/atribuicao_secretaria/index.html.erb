<p id="notice"><%= notice %></p>

<h1>Professores</h1>

<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Registro</th>
      <th>Nota prova</th>
      <th>Categoria</th>
      <th ></th>
    </tr>
  </thead>

  <tbody>
    <% @professores.each do |professor| %>
      <tr>
        <td><%= professor.nome %></td>
        <td><%= professor.registro %></td>
        <td><%= professor.nota_prova %></td>
        <td><%= professor.categoria.descricao %></td>
        <td><%= link_to 'Atribuir Sala', edit_atribuicao_secretarium_path(professor) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
